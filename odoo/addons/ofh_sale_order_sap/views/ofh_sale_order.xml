<odoo>
    <record model="ir.ui.view" id="ofh_sale_order_form_view">
        <field name="name">ofh.sale.order.form (in ofh_sale_order_sap)</field>
        <field name="model">ofh.sale.order</field>
        <field name="inherit_id" ref="ofh_sale_order.ofh_sale_order_form_view"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="sap_details" string="SAP Details">
                    <field name="sale_order_ids" nolabel="1"/>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
